<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uv.fei.polinizadores.Login">
    <children>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="44.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true" />
