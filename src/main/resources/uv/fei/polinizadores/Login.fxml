            <font>
                <Font size="28.0" />
            </font></Label>
        <Label layoutX="50.0" layoutY="105.0" text="Correo Electronico">
            <font>
                <Font size="14.0" />
            </font></Label>
        <Label layoutX="51.0" layoutY="175.0" text="Contraseña">
            <font>
                <Font size="14.0" />
            </font></Label>
        <Label layoutX="50.0" layoutY="314.0" text="¿Necesitas una cuenta?" />
        <TextField fx:id="fieldEmail" layoutX="51.0" layoutY="131.0" onAction="#emailAction" prefHeight="25.0" prefWidth="186.0" promptText="email" />
        <Button fx:id="buttonLogin" layoutX="52.0" layoutY="278.0" mnemonicParsing="false" onAction="#buttonLoginClic" prefHeight="25.0" prefWidth="186.0" style="-fx-background-color: #001CFF; " text="Entrar" textFill="WHITE">
            <font>
                <Font size="14.0" />
            </font></Button>
        <Button fx:id="buttonEntrarInvitado" layoutX="342.0" layoutY="278.0" mnemonicParsing="false" onAction="#buttonEntrarInvitadoClic" prefHeight="30.0" prefWidth="186.0" style="-fx-background-color: #001CFF;" text="Entrar como invitado" textFill="WHITE">
