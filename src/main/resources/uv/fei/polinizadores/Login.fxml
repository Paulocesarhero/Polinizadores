        <TextField fx:id="passwordField" layoutX="53.0" layoutY="205.0" prefHeight="25.0" prefWidth="186.0" visible="false" />
      <Button fx:id="buttonRegister" layoutX="177.0" layoutY="310.0" mnemonicParsing="false" onAction="#buttonRegisterClic" style="-fx-background-color: #FFFFFF;" text="Registrate">
